    <text x="90" y="430" font-family="Arial" font-size="14" fill="#6b7280">Speed: 5 | Weight: 180g</text>
    
    <rect x="20" y="460" width="260" height="60" rx="10" fill="white" stroke="#e5e7eb" />
    <circle cx="50" cy="490" r="20" fill="#ef4444" />
    <text x="90" y="480" font-family="Arial" font-size="16" font-weight="bold" fill="#111827">Putter</text>
    <text x="90" y="500" font-family="Arial" font-size="14" fill="#6b7280">Speed: 2 | Weight: 173g</text>
    
    <!-- Bottom Navigation -->
    <rect x="0" y="550" width="300" height="50" fill="white" stroke="#e5e7eb" />
    <circle cx="75" cy="575" r="15" fill="#4f46e5" opacity="0.2" />
    <text x="75" y="580" font-family="Arial" font-size="18" text-anchor="middle" fill="#4f46e5">🏠</text>
    <text x="150" y="580" font-family="Arial" font-size="18" text-anchor="middle" fill="#6b7280">🔍</text>
    <text x="225" y="580" font-family="Arial" font-size="18" text-anchor="middle" fill="#6b7280">👤</text>
</svg>
