    <!-- Disc Section -->
    <text x="30" y="300" font-family="Arial" font-size="18" font-weight="bold" fill="#111827">My Discs</text>
    
    <!-- Disc Items -->
    <rect x="20" y="320" width="260" height="60" rx="10" fill="white" stroke="#e5e7eb" />
    <circle cx="50" cy="350" r="20" fill="#4f46e5" />
    <text x="90" y="340" font-family="Arial" font-size="16" font-weight="bold" fill="#111827">Distance Driver</text>
    <text x="90" y="360" font-family="Arial" font-size="14" fill="#6b7280">Speed: 12 | Weight: 175g</text>
    
    <rect x="20" y="390" width="260" height="60" rx="10" fill="white" stroke="#e5e7eb" />
    <circle cx="50" cy="420" r="20" fill="#10b981" />
